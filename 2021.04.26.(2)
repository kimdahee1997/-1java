    package week1;

public class dahee {
	public static void main(String[] args) {
		char a = 'a';
		int count = 0;
		do {
			System.out.print(a);
			a = (char) (a+1);
			count++;
		} while (a <= 'z');
		System.out.println("\n"+count);
	}	
}		
		
    
